page {
  background-color: #EEEEEE;
  display: flex;
  flex-direction: column;
}
.qs-button {
  width: 150px;
  height: 40px;
  margin-top: 10px;
  border-radius: 50px;
  color: white;
  text-align: center;
  font-size: 16px;
}
.button-hover {
  background: rgba()
}
.qs-list {
  margin-top: 20px;
}
.qs-item {
  background-color: #FFFFFF;
  height: 80px;
  margin: 15px 8px 15px 8px;
  border-radius: 5px;
  box-shadow: 0px 1px 2px 1px #CCCCCC;
  position: relative;
}
.qs-item-head {
  height: 32px;
  border-top-left-radius: 5px;
  border-top-right-radius: 5px;
  background: #E6F0FA;
  line-height: 30px;
  padding: 4px;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
}
.qs-item-body {
  height: 36px;
  padding: 4px;
  line-height: 30px;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
}

::-webkit-scrollbar {
width: 4px;
height: 6px;
background-color: #ffffff;
}
/*定义滚动条轨道 内阴影+圆角*/
::-webkit-scrollbar-track
{
  /*-webkit-box-shadow: inset 0 0 10px rgba(0,0,0,0.3);*/
  border-radius: 10px;
  background-color: #EEEEEE;
  /*display: none;*/
}
/*定义滑块 内阴影+圆角*/
::-webkit-scrollbar-thumb
{
border-radius: 10px;
-webkit-box-shadow: inset 0 0 10px rgba(0,0,0,.3);
background-color: #EEEEEE;
}
